{
  "name": "mjml-api",
  "version": "1.0.0",
  "description": "MJML API Conversion",
  "repository": "https://github.com/ViBiOh/mjml-api",
  "main": "dist/index.js",
  "scripts": {
    "init": "curl -q -sSL --max-time 10 'https://raw.githubusercontent.com/ViBiOh/scripts/master/bootstrap' | bash -s 'git_hooks'",
    "format": "prettier --write src/*.js",
    "style": "eslint --fix src/",
    "prebuild": "npm run format && npm run style",
    "build": "babel src --out-dir dist",
    "start": "babel-node src/index.js"
  },
  "keywords": [
    "mjml",
    "api",
    "conversion"
  ],
  "author": "ViBiOh",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "jaeger-client": "3.17.1",
    "mjml": "4.5.1",
    "yargs": "15.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.7.7",
    "@babel/core": "7.7.7",
    "@babel/node": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@babel/register": "7.7.7",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "prettier": "1.19.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
