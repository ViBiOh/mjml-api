{
  "name": "mjml-api",
  "version": "1.0.0",
  "description": "MJML API Conversion",
  "repository": "https://github.com/ViBiOh/mjml-api",
  "main": "dist/index.js",
  "scripts": {
    "init": "curl --disable --silent --show-error --location --max-time 30 'https://raw.githubusercontent.com/ViBiOh/scripts/main/bootstrap.sh' | bash -s 'git_hooks'",
    "format": "./node_modules/prettier/bin/prettier.cjs --write \"src/**/*.mjs\"",
    "style": "./node_modules/eslint/bin/eslint.js --fix \"src/**/*.mjs\"",
    "start": "node src/index.mjs"
  },
  "keywords": [
    "mjml",
    "api",
    "conversion"
  ],
  "author": "ViBiOh",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "1.8.0",
    "@opentelemetry/auto-instrumentations-node": "0.45.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.51.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.51.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.51.0",
    "@opentelemetry/exporter-trace-otlp-proto": "0.51.0",
    "@opentelemetry/instrumentation-http": "0.51.0",
    "@opentelemetry/resources": "1.24.0",
    "@opentelemetry/sdk-metrics": "1.24.0",
    "@opentelemetry/sdk-node": "0.51.0",
    "@opentelemetry/semantic-conventions": "1.24.0",
    "body-parser": "1.20.2",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "mjml": "4.15.3",
    "winston": "3.13.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "eslint": "9.1.1",
    "prettier": "3.2.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
